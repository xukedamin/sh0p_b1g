<ul class="product-pagi-nav pull-right list-inline">

  {% if paginate.previous %}
   <li class="item-paginate">  <a  href="{{ paginate.previous.url }}" title="{{ paginate.previous.title }}"><i class="lnr-chevron-left"></i></a></li>
  {% else %}
  <li  class="item-paginate disabled">  <span ><i class="lnr-chevron-left"></i></span></li>
  {% endif %}

  {% for part in paginate.parts %}
    {% if part.is_link %}

       <li class="item-paginate" >  <a href="{{ part.url }}" title="">{{ part.title }}</a></li>

    {% else %}
      {% if part.title == paginate.current_page %}
     <li class="item-paginate active">  <span >{{ part.title }}</span></li>
      {% else %}
     <li  class="item-paginate">  <span>{{ part.title }}</span></li>
      {% endif %}
    {% endif %}
  {% endfor %}

  {% if paginate.next %}
    <li class="item-paginate"> <a href="{{ paginate.next.url }}" title="{{ paginate.next.title }}"><i class="lnr-chevron-right"></i></a></li>
  {% else %}
   <li class="item-paginate disabled"> <span ><i class="lnr-chevron-right"></i></span></li>
  {% endif %}

</ul>

