<main class="page-home-home3">
  <header class="home-sticky-nav-trigger">
    <div class="home-fw-slider style-6 caption-slider dir-nav parents-height">

      {% capture slider %}slider-{{settings.home3_slide_style}}{% endcapture %}
      {% include slider  %}

    </div>
  </header>
  {% comment %} ----- Block Who We Are ----- {% endcomment %}
  {% if settings.home3_who_we_are %}   
  <section>
    <header class="section-header header-type-1 style-2">
      <div class="container">
        <div class="row">
          <div class="col-lg-10">
            <h5>{{  settings.home3_who_we_are_heading_1 }}</h5>
            <h2 >{{  settings.home3_who_we_are_heading_2 }}</h2>

            {% if settings.home3_who_we_are_heading_icon != blank %}
            <div class="heading-icon-section {{  settings.home3_who_we_are_heading_icon }}"></div>
            {% endif %}              


            <p class="header-caption">{{  settings.home3_who_we_are_text }}</p>
          </div>
        </div>
      </div>
    </header>
    <div class="container">
      <div class="row">

        {% for i in (1..3) %}
        {% capture who_we_are_title_1 %}home3_offer_caption_{{ i }}_1{% endcapture %}
        {% capture who_we_are_title_2 %}home3_offer_caption_{{ i }}_2{% endcapture %}
        {% capture who_we_are_text %}home3_offer_text_{{ i }}{% endcapture %}
        {% capture who_we_are_link %}home3_offer_link_{{ i }}{% endcapture %}
        {% capture who_we_are_img %}about_0{{ i }}.jpg{% endcapture %}

        <div class="col-md-4 section-block-p">
          <div class="about-item-01">
            <a href="{% if settings[who_we_are_link] == blank %} http://themeforest.net/user/joomlavi/portfolio {% else %} {{ settings[who_we_are_link] }} {% endif %}">
              <div>

                <img src="{{ who_we_are_img | asset_url }}"  alt="Brand image" >
                <h6>{{ settings[who_we_are_title_1] }}</h6>
                <h4>{{ settings[who_we_are_title_2] }}</h4>
                <p>{{ settings[who_we_are_text] }}</p>
              </div>
            </a></div>
        </div>        

        {% endfor %}        

      </div>
    </div>
  </section>
  {% endif %} 

  {% comment %} ----- Block Quote ----- {% endcomment %}
  {% if settings.home3_quote %}  
  <section class="one-child">
    <div data-stellar-background-ratio="0.25" style="background-image: url('{{ 'home3_quote.jpg' | asset_url }}');" class="parallax-bg quote-slider-parallax"></div>
    <div class="container">
      <div class="row">
        <div class="col-xs-12 text-center remove-col-padding-2">
          <div class="quote-slider-01">

            <div class="quote-item">
              <div class="item-content">
                <p class="color-light">&quot; {{ settings.home3_quote_text }} &quot;</p>
                {% if settings.home3_quote_heading_icon != blank %}
                <div class="text-center  color-light heading-icon-section  {{  settings.home3_quote_heading_icon }}"></div>
                {% endif %}   

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  {% endif %} 
  {% comment %} ----- Block Portfolio ----- {% endcomment %}
  {% if settings.home3_portfolio_enable %}  
  <section>
    <header class="section-header header-type-1 style-1">
      <div class="container">
        <div class="row">
          <div class="col-lg-10 col-lg-offset-1">

            {% if settings.home3_portfolio_heading_icon != blank %}
            <div class="heading-icon-section  {{  settings.home3_portfolio_heading_icon }}"></div>
            {% endif %}     
            <h5 >{{ settings.home3_portfolio_heading_1 }}</h5>
            <h2>{{ settings.home3_portfolio_heading_2 }}</h2>
            <p class="header-caption">{{ settings.home3_portfolio_text }}</p>
          </div>
        </div>
      </div>
    </header>

    {% if settings.home3_portfolio %}  
    <div class="homepage-portfolio-special-group section-block-p">
      <div data-filter-nav="#portfolio-external-filter" class="isotope-container">
        <div class="portfolio-group container">
          <div class="row iso-grid">

            {% include 'jv-portfolio' %}

          </div>
        </div>
      </div>
    </div>
    {% endif %} 


  </section>
  {% endif %} 


  {% comment %} ----- Block PURCHASE ----- {% endcomment %}
  {% if settings.home3_purchase_enable %}  
  <section class="no-padding">
    <div class="call-to-action-wrapper style-2">
      <div class="container">
        <div class="call-to-action">
          <p>{{  settings.home3_purchase_text }}  </p>
          <a href="{{  settings.home3_purchase_link }}" class="normal-btn normal-btn-main btn-size-5"> {{ settings.home3_purchase_button }}</a> </div>
      </div>
    </div>
  </section>
  {% endif %} 
  {% comment %} ----- Block CREATIVITY ----- {% endcomment %}
  {% if settings.home3_creativity_enable %}  
  <section>
    <header class="section-header header-type-1 style-2">
      <div class="container">
        <div class="row">
          <div class="col-lg-10">
            <h5>{{  settings.home3_creativity_heading_1 }} </h5>
            <h2 >{{  settings.home3_creativity_heading_2 }} </h2>
            {% if settings.home3_creativity_heading_icon != blank %}
            <div class="heading-icon-section  {{  settings.home3_creativity_heading_icon }}"></div>
            {% endif %}               
            <p class="header-caption">{{  settings.home3_creativity_text }} </p>
          </div>
        </div>
      </div>
    </header>
    <div class="service-section-01">
      <div class="container">
        <div class="row">
          <div class="col-lg-6 image-container section-block">
            <img src="{{ 'service_01.jpg' | asset_url }}" alt="service">
          </div>
          <div class="col-lg-6 content-container section-block">
            {% include 'jv-tabs-2' %}
            <div class="service-button"><a href="{{  settings.home3_creativity_link }}" class="border-btn border-btn-main btn-size-5">{{  settings.home3_creativity_button }}</a></div>
          </div>
        </div>
      </div>
    </div>
  </section>
  {% endif %}  


  {% comment %} ----- Block Testimonial ----- {% endcomment %}
  {% if settings.home3_testimonial_enable %}  
  <section class="one-child">
    <div data-stellar-background-ratio="0.25" style="background-image: url('{{ 'home3_testimonial.jpg' | asset_url }}');" class="parallax-bg quote-slider-parallax"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-10 col-lg-offset-1 col-xs-12 text-center color-light">
          <h4 class="testimonial-heading-01 hr-light">  {{  settings.home3_testimonial_heading }}   </h4>
          <div class="testimonial-01 simple-slider">

            {% include 'jv-testimonials-2' %}



          </div>
        </div>
      </div>
    </div>
  </section>
  {% endif %} 

  {% comment %} ----- Block Team ----- {% endcomment %}
  {% if settings.home3_team_enable %}  
  <section>
    <header class="section-header header-type-1 style-2">
      <div class="container">
        <div class="row">
          <div class="col-lg-10">
            <h5>{{  settings.home3_team_heading_1 }}</h5>
            <h2 >{{  settings.home3_team_heading_2 }}</h2>
            {% if settings.home3_team_heading_icon != blank %}
            <div class="heading-icon-section  {{  settings.home3_team_heading_icon }}"></div>
            {% endif %}              
            <p class="header-caption">{{  settings.home3_team_text }}</p>
          </div>
        </div>
      </div>
    </header>
    <div class="team-section-02 text-center">
      <div class="container">
        <div class="row">

          {% include 'jv-team' %}
        </div>
      </div>
    </div>
  </section>
  {% endif %}  


  {% comment %} ----- Block Partner ----- {% endcomment %}
  {% if settings.home3_partner_enable %}  
  <section  class=" one-child logo-partner">
    <div class="container">
      <div class="brand-carousel multi-slider">
        {% include 'jv-logopartner' %}
      </div>
    </div>
  </section>
  {% endif %}  

  {% comment %} ----- Home Blog ----- {% endcomment %}
  {% if settings.home3_blog_enable %}   
  <section class="blog-lastest-post">
    <div class="container">
      <header class="section-header header-type-1 style-2">   



        <div class="row">
          <div class="col-lg-10">
            {% if settings.home3_blog_heading_1 != blank %}
            <h5>{{ settings.home3_blog_heading_1 }}</h5>
            {% endif %}
            {% if settings.home3_blog_heading_2 != blank %}
            <h2 >{{ settings.home3_blog_heading_2 }}</h2>
            {% if settings.home3_blog_heading_icon != blank %}
            <div class="heading-icon-section  {{  settings.home3_blog_heading_icon }}"></div>
            {% endif %}              
            {% endif %}
            {% if settings.home3_blog_text != blank %}
            <p class="header-caption">{{ settings.home3_blog_text }}</p>
            {% endif %}
          </div>
        </div>            


      </header>  
      <div class="row">


        {% assign tmp = settings.home3_blog %}
        {% assign blog = blogs[tmp] %}
        {% for article in blog.articles limit:settings.home3_blog_number %}

        <div class="col-md-4 col-xs-12">
          <div class="blog-item section-block-p">

            {% if article.excerpt.size > 0 %}

            <div class="item-image">
              {{ article.excerpt }}
            </div>
            {% else %}
            {% if article.image %}
            {% assign image_alt = article.title | escape %}

            <div class="item-image">

              {{ article | img_url: '1024x1024' | img_tag: image_alt, 'article__image' | link_to: article.url }}

            </div>

            {% endif %}    
            {% endif %} 
            <p><a href="{{ blog.url }}">{{ blog.title }}</a>&nbsp;/&nbsp;<time  datetime="{{ article.published_at | date: '%Y-%m-%d' }}">{{ article.published_at | date: format: 'month_day_year' }}</time></p>
            <h5><a href="{{ article.url }}">{{ article.title }}</a></h5>
          </div>
        </div>
        {% endfor %}
      </div>
    </div>
  </section>      

  {% endif %}   



</main>